#!groovy

@Library("Infrastructure")

def type = "java"
def product = "hmi"
def component = "rota-dtu"

withPipeline(type, product, component) {

  onMaster() {
    env.ENV = 'stg'
  }
  onPR() {
    env.ENV = 'dev'
  }
  onDemo {
    env.ENV = 'demo'
  }
  onPerftest {
    env.ENV = 'perftest'
  }
  onIthc {
    env.ENV = 'ithc'
  }

  enableSlackNotifications('#hmi-build-notices')
  nonServiceApp()
}